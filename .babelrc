{
  //emotion needs to be the first plugin the array
  "plugins": [
    "emotion",
    "react-hot-loader/babel",
    [
      "@emotion/babel-plugin-jsx-pragmatic",
      {
        "module": "theme-ui",
        "import": "jsx",
        "export": "jsx"
      }
    ],
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "jsx",
        "pragmaFrag": "React.Fragment"
      }
    ]
  ],
  "presets": ["@babel/env", "@babel/preset-react"]
}