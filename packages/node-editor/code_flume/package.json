{
  "name": "builder",
  "version": "0.0.1",
  "description": "A node editor for React",
  "author": "philgarb",
  "license": "MIT",
  "repository": "open-decision/flume",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom && npx cypress run",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@svgr/rollup": "^5.5.0",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/react": "^11.2.2",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-portal": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.x",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^7.0.3",
    "cypress": "^6.0.1",
    "eslint": "7.x",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-flowtype": "5.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.x",
    "eslint-plugin-standard": "^4.1.0",
    "jest-puppeteer": "^4.4.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "rollup": "^2.34.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-url": "^1.4.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "typescript-plugin-css-modules": "^3.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@reach/auto-id": "^0.12.1",
    "@types/d3-shape": "^2.0.0",
    "d3-shape": "^2.0.0",
    "eslint-config-prettier": "^7.0.0",
    "immer": "^8.0.0",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.20",
    "react-portal": "^4.2.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier",
      "prettier/react",
      "prettier/@typescript-eslint",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:react-hooks/recommended"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-return": "off",
      "@typescript-eslint/no-explicit-any": "off"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "ignorePatterns": [
      "src/types/generated/*",
      "*.config*"
    ]
  },
  "prettier": {}
}
